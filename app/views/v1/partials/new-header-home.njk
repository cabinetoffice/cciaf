<style>

    .govuk-header__container {
        border-bottom: 0;
    }
</style>

<style>

    .s4_top-nav__menu {
        display: flex;
        flex-direction: column;

    }

    .s4_top-nav__item-list {
        position: relative;
        display: flex !important;
        height: 36px;
        flex-direction: row;
        right: 0;
    }

    .s4_top-nav__item {
        display: inline-flex;
        padding: 0;
        border: 0;
    }

    .s4_top-nav__item > a {
        display: inline-flex;
        line-height: 36px;
        letter-spacing: 0.2px;
        color: #ffffff;
        text-decoration: none;
    }

    .s4_top-nav__item.s4_top-nav__item--active {
        background-color: #fafafa;
        padding: 0 12px;
        color: #000000;
        flex-direction: column;
        flex: 0 1 auto;
    }

    .s4_top-nav__item.s4_top-nav__item--active .s4_top-nav__sub-item-list {
        display: flex;
    }

    .s4_top-nav__item.s4_top-nav__item--active a {
        color: #000000;
        flex-direction: column;
        flex: 1;
        justify-content: center;
    }

    .s4_top-nav__sub-item-list {
        position: absolute;
        top: 36px;
        list-style: none;
        padding-left: 0;
        left: 0;color: #000000;
        align-items: stretch;
        display: none;
        right: 0;
    }

    .s4_top-nav__sub-item {
        padding: 0 12px;
        display: flex;
        /*flex: 1;*/
        flex-direction: column;
        height: 40px;
        /*margin: 0 10px;*/
    }

    .s4_top-nav__sub-item:first-of-type {
        padding-left: 0;
    }

    .s4_top-nav__sub-item a {
        text-decoration: none;
        display: flex;
        flex-direction: column;
        flex: 1;
        line-height: 40px;
        font-weight: 400;
        color: #1d70b8;
    }

    .s4_top-nav__sub-item.s4_top-nav__sub-item--active > a {
        text-decoration: none;
        display: flex;
        flex-direction: column;
        flex: 1;
        line-height: 40px;
        box-shadow: inset 0 -4px 0 rgb(29 112 184);
        padding-bottom: 1px;
        color: #1a1a1a;
        font-weight: 600;
    }

    .s4_top-nav__sub-item-bar {
        background-color: #fafafa;height: 40px;border-bottom: 1px solid #dadada;
    }

    .s4_top-nav__sub-item-bar.s4_top-nav__sub-item-bar--hidden {
        height: 0;
    }


    .gap_new-top-nav {
        display: flex;flex-direction: column;
    }

    .gap_new-top-nav__list {
        position: relative;display: flex;height: 36px;flex-direction: row;
    }

    /*.gap_new-top-nav__list li:fir {*/

    /*}*/

    .plus-test::after {
        display: inline-block;
        vertical-align: middle;
        border-radius: 50%;
        width: 16px;
        height: 16px;
        background-image: linear-gradient(black, black), linear-gradient(black, black);
        background-repeat: no-repeat;
        background-position: center center;
        background-size: 20% 70%, 70% 20%;
        position: relative;
        margin-left: 6px;
        top: -2px;
        content: "";
    }

</style>


