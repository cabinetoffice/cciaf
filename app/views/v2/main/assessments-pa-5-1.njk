





<div class="govuk-grid-row govuk-!-margin-top-7">


    <style>
        .s4_nav-test {
            display: flex;
            line-height: 30px;
            margin-bottom: 6px;
        }

        .s4_nav-link-test {
            padding-left: 14px;
            display: flex;
            flex: 1;
        }

        .s4_nav-link-test--active {
            margin-right: -12px;
        }

        .s4_active-test {
            font-weight: 600;
            text-decoration: none;
            color: #000 !important;
            list-style-type: none;
            border-left: 5px solid #1d70b8;
            display: flex;
            /*margin-left: -20px;*/
            padding-left: 14px;
            background-color: #e3edff;
            padding-top: 5px;
            padding-bottom: 5px;
            flex: 1;
        }

        .s4_table-head {
            background-color: #f7f7f7;
            border: 1px solid #dadada;
            border-bottom: 2px solid #000;
        }
    </style>

    <style>

        .gc_bold-text {
            font-weight: 600;
        }

        .gc_red-text {
            color: #d4351c;
        }

        .gc_orange-text {
            color: #f47738;
        }

        .gc_green-text {
            color: #005a30;
        }

        .gc_blue-text {
            color: #1d70b8;
        }


    </style>



    {% include '../nav/left-nav-assessment-pa-5-1.njk' %}


    <!-- START: RHS CONTENT @80% -->
    <div class="govuk-!-margin-bottom-9 govuk-grid-column-three-quarters" style="width: 80%;">

        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-xl ">Practice Area 5.1</h1>

            </div>
        </div>


        <div class="govuk-grid-row">

            <ul class="" style="list-style: none;padding-left: 15px;">

                {% for item in urlObj.data %}

                    <li class="">
                        <span class="govuk-heading-s">
                            {{ item.pa_id|safe }}&nbsp;&nbsp;{{ item.pa_desc|safe }}
                        </span>
                    </li>

                    <div class="govuk-form-group">
                        <fieldset class="govuk-fieldset">
                            {#
                            <legend class="govuk-fieldset__legend govuk-fieldset__legend--m">
                            <h1 class="govuk-fieldset__heading">
                            Filter
                            </h1>
                            </legend>
                            #}
                            <div class="govuk-radios govuk-radios--small govuk-radios--inline" data-module="govuk-radios">
                                <!-- START: FULLY MEETING -->
                                <div class="govuk-radios__item">
                                    <input class="govuk-radios__input"
                                           id="changed-name" name="changed-name" type="radio" value="month"
                                           data-aria-controls="conditional-contact">
                                    <label class="govuk-label govuk-radios__label" for="changed-name">
                                        Fully meeting
                                    </label>
                                </div>
                                <!-- END: FULLY MEETING -->
                                <div class="govuk-radios__item">
                                    <input class="govuk-radios__input" id="changed-name-2" name="changed-name" type="radio" value="year">
                                    <label class="govuk-label govuk-radios__label" for="changed-name-2">
                                        Partially meeting
                                    </label>
                                </div>
                                <div class="govuk-radios__divider">or</div>
                                <div class="govuk-radios__item">
                                    <input class="govuk-radios__input" id="changed-name-2" name="changed-name" type="radio" value="year">
                                    <label class="govuk-label govuk-radios__label" for="changed-name-2">
                                        Not or seldom meeting
                                    </label>
                                </div>
                            </div>



                                <div class="govuk-form-group govuk-!-margin-top-5">
                                    {#
                                    <h1 class="govuk-label-wrapper"><label class="govuk-label govuk-label--l" for="more-detail">
                                    Can you provide more detail?
                                    </label>
                                    </h1>
                                    #}
                                    <div id="more-detail-hint" class="govuk-hint">
                                        Do not include personal or financial information, like your National Insurance number or credit card details.
                                    </div>
                                    <textarea class="govuk-textarea" id="more-detail" name="more-detail" rows="5" aria-describedby="more-detail-hint"></textarea>
                                </div>

                                <div class="govuk-form-group">
                                    <label class="govuk-label" for="file-upload-1">
                                        Upload a file
                                    </label>
                                    <input class="govuk-file-upload" id="file-upload-1" name="file-upload-1" type="file">
                                </div>





                        </fieldset>

                    </div>

                    <hr class="govuk-section-break govuk-section-break--l govuk-section-break--visible">

                {% endfor %}

            </ul>

        </div>


    </div>
    <!-- END: RHS CONTENT @80% -->


    <!-- START:    --->

</div>
